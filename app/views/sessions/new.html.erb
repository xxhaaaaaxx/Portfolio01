<h1>ログイン</h1>

<%= form_with scope: :session, local: true do |f| %>

    <div class="field">
        <%= f.label :email %><br>
        <%= f.text_field :email %><br>
    </div>

    <div class="field">
        <%= f.label :password %><br>
        <%= f.text_field :password %><br>
    </div>

    <div class="actions">
        <%= f.submit('ログイン') %>
    </div>

    <%= link_to '>>> 新規ユーザー登録', new_user_path %>
    
<% end %>